#%Module1.0#####################################################################
##
## git modulefile
##

set             release                 25.2
set             devroot                 $::env(DEVROOT)
set             path                    $devroot/install/emacs

prepend-path    PATH                    $path/bin
prepend-path    MANPATH                 $path/share/man/

module-whatis "Sets up environment for using Emacs ${release}"

proc ModulesHelp { } {
  global release
  puts stderr "\tAdds directories for using Emacs ${release} to your path"
}
